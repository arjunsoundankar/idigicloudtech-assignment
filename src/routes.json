[
    {
      "name":"Login",
      "locale":"user.login",
      "path":"/user/login",
      "component":"./User/Login",
      "hideInMenu":true,
      "icon": ""
    },
    {
      "path":"/dashboard",
      "locale":"dashboard",
      "name":"Dashboard",
      "icon":"dashboard",
      "exact":true,
      "component":"./pages/dashboard/dashboard",
      "routes":[
        {
          "path":"/dashboard/analysis",
          "locale":"dashboard.analysis",
          "name":"analysis",
          "component":"./pages/dashboard/analysis",
          "exact":true,
          "icon":"analysis",
          "accessTO":[
            "admin"
          ]
        },
        {
          "path":"/dashboard/monitor",
          "locale":"dashboard.monitor",
          "component":"./pages/dashboard/monitor",
          "name":"monitor",
          "icon":"monitor",
          "exact":true
        },
        {
          "path":"/dashboard/workplace",
          "locale":"dashboard.workplace",
          "component":"./pages/dashboard/workplace",
          "name":"workplace",
          "icon":"workplace",
          "exact":true
        }
      ]
    },
    {
        "path":"/projects",
        "locale":"projects",
        "name":"Projects",
        "icon":"projects",
        "redirect":"/projects/list",
        "routes":[
          {
            "path":"/projects/list",
            "locale":"projects.list",
            "name":"Projects List",
            "icon":"list",
            "component":"./pages/projects/projectslist",
            "exact":true
          },
          {
            "path":"/projects/:id",
            "locale":"projects.details",
            "name":"Project Details",
            "hideInMenu":true,
            "icon":"details",
            "key":"projects",
            "component":"./pages/projects/projectsdetails",
            "exact":true
          },
          {
            "path":"/projects/:id/settings",
            "locale":"projects.settings",
            "icon":"settings",
            "name":"Settings",
            "parentKey":"details",
            "component":"./pages/projects/projectssettings",
            "exact":true
          }
        ]
      },
    {
      "path":"*",
      "component":"./404"
    }
]